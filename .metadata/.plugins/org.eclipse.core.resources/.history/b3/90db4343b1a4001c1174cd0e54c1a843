package cap3que2;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int A, B, C;
		
		System.out.print("Digite os valores de A, B e C ");
		A = sc.nextInt();
		B = sc.nextInt();
		C = sc.nextInt();
		
		if(A == B && B == C && C == A) {
			System.out.print("Numeros iguais");
		}
		else if(A > B && A > C && B > C) {
			System.out.printf(" a ordem e %d, %d, %d: ", A,B,C);
		}else {
			System.out.printf("A ordem e %d, %d, %d: ", A, C, B);
		}
	sc.close();
	}

}
