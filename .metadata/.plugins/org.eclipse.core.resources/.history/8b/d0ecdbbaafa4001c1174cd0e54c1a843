package cap3que2;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int A, B, C;
		
		System.out.print("Digite os valores de A, B e C ");
		A = sc.nextInt();
		B = sc.nextInt();
		C = sc.nextInt();
		
		if(A == B && B == C) {
			System.out.print("Numeros iguais");
		}
		else if(A > B && A > C && B > C) {
			System.out.printf("", A, B, C);
		}
		else {
			System.out.printf("", A, C, B);
		}
		if(B > A && B > C && A > C) {
			System.out.printf("", B, A, C);
		}
		else {
			System.out.printf("", C, B, A);
		}
		if(C > A && C > B && A > B) {
			System.out.printf("", C, A, B);
		}
		else {
			System.out.printf("", C, B, A);
		}
	sc.close();
	}

}
